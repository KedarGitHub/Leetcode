   "source": [
    "def dominantIndex(nums):\n",
    "    maximum = max(nums)\n",
    "    index = nums.index(maximum)\n",
    "    nums.remove(maximum)\n",
    "    second = max(nums)\n",
    "    if maximum >= 2*second:\n",
    "        return index\n",
    "    else:\n",
    "        return -1"
   ]
